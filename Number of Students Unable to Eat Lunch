class Solution:
    def countStudents(self, students: List[int], sandwiches: List[int]) -> int:
        queue=collections.deque(students)
        for sand in sandwiches:
            if sand in queue:
                while queue[0]!=sand:
                    x=queue.popleft()
                    queue.append(x)
                queue.popleft()
            else:
                return len(queue)
        return 0
                    
        
